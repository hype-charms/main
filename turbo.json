{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "prebuild": {
            "cache": false,
            "dependsOn": [
                "^lint",
                "^prebuild"
            ],
            "outputs": [
                "dist"
            ]
        },
        "build": {
            "dependsOn": [
                "^lint",
                "^prebuild",
                "^build"
            ],
            "outputs": [
                ".next/**",
                ".build/**"
            ]
        },
        "interface:build": {
            "dependsOn": [
                "^lint",
                "^prebuild",
                "^build"
            ],
            "outputs": [
                ".next/**",
                ".build/**"
            ]
        },
        "admin:build": {
            "dependsOn": [
                "^lint",
                "^prebuild",
                "^build"
            ],
            "outputs": [
                ".next/**",
                ".build/**"
            ]
        },
        "postinstall": {
            "dependsOn": [
                "^lint",
                "^prebuild",
                "^build"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "interface:dev": {
            "cache": false,
            "persistent": true
        },
        "admin:dev": {
            "cache": false,
            "persistent": true
        },
        "playground:dev": {
            "cache": false,
            "persistent": true
        },
        "lint": {},
        "interface:start": {
            "dependsOn": [
                "^build"
            ]
        },
        "admin:start": {
            "dependsOn": [
                "^build"
            ]
        }
    }
}